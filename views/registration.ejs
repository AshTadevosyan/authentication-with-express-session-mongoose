<body>
   <section>
        <h1>Registarion</h1>
      <form method="POST" action="/register">
          <label for="exampleInputEmail1">First name</label>
          <input type="name"  name="name" placeholder="Enter first name"></br>
          <label for="exampleInputEmail1">Username</label>
          <input type="username" name="username" id="exampleInputEmail1" placeholder="Enter username"></br>
          <label for="exampleInputPassword1">Password</label>
          <input type="password" name="pwd" id="exampleInputPassword1" placeholder="Enter Password"></br>
          <label for="exampleInputPassword2">Confirm Password</label>
          <input type="password" name="pwd2" id="exampleInputPassword2" placeholder="Confirm Password"></br>
          <button type="submit" value="Submit">Register</button>
      </form>
     <a href="/login">Login</a>
    </section>
